{
	(import "msgpack.bin")

	# buffer
	(let sbuf (msgpackSBuffer))

	# packing
	(msgpackPack sbuf (list 1 2.45 "Yellow Bow !" true nil false))
	
	# unpacking
	# msgpack object handle
	(let oh (msgpackUnpack sbuf))
	
	# msgpack object
	(let o (msgpackObj oh))
	(printO o)

	# convert msgpackObj to ark object
	(mut dst (msgpackConvert o))
	(print "dst : " dst)
}